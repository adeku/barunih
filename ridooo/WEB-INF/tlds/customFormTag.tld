			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Add class for label</description>
			<name>labelclass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Hour Class</description>
			<name>hourclass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
